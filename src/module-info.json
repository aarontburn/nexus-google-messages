{
    "name": "Google Messages",
    "id": "aarontburn.Google_Messages",
    "author": "aarontburn",
    "version": "1.0.0",
    "description": "A Google Messages embed for Nexus",
    "platforms": [
        "win32",
        "linux",
        "darwin"
    ],
    "link": "https://github.com/aarontburn/nexus-google-messages",
    "author-id": "683193056e5989838dcf772c",
    "git-latest": {
        "git-username": "aarontburn",
        "git-repo-name": "nexus-google-messages"
    },
    "build": {
        "build-version": 3,
        "excluded": [
            "electron.ts"
        ],
        "included": [],
        "process": "./process/main.js",
        "replace": [
            {
                "from": "{EXPORTED_MODULE_ID}",
                "to": "%id%",
                "at": [
                    "./process/main.ts"
                ]
            },
            {
                "from": "{EXPORTED_MODULE_NAME}",
                "to": "%name%",
                "at": [
                    "./process/main.ts"
                ]
            }
        ]
    }
}